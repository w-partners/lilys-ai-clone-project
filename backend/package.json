{
  "name": "lilys-ai-clone-backend",
  "version": "1.0.0",
  "description": "Backend API for Lilys.AI clone - AI content processing platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "db:create": "node scripts/createDatabase.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:setup": "npm run db:create && npm run db:migrate && npm run db:seed",
    "accounts:init": "node scripts/initializeAccounts.js",
    "accounts:check": "node scripts/checkAccounts.js",
    "accounts:reset": "node scripts/resetAccounts.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@google/generative-ai": "^0.2.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.5",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^21.6.1",
    "sequelize": "^6.35.2",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "youtube-transcript": "^1.2.1",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.56.0",
    "eslint-config-node": "^4.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "ai",
    "content-processing",
    "summarization",
    "openai",
    "gemini",
    "nodejs",
    "express",
    "api"
  ],
  "author": "Starian AI",
  "license": "MIT"
}
